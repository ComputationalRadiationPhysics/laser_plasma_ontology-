    <!-- http://www.semanticweb.org/afshar87/ontologies/2024/3/Laser-Plasma_ontology#laser_pulse -->

    <owl:Class rdf:about="http://www.semanticweb.org/afshar87/ontologies/2024/3/Laser-Plasma_ontology#laser_pulse">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/afshar87/ontologies/2024/3/Laser-Plasma_ontology#Laser"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/afshar87/ontologies/2024/3/Laser-Plasma_ontology#has_pulse_duration"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/afshar87/ontologies/2024/3/Laser-Plasma_ontology#laser_pulse"/>
